[Autostart]
match := True
startup := Shell "xrdb -DSOLARIZED_DARK -merge ${XDG_CONFIG_HOME}/xorg/db"
startup := Shell "xsetroot -cursor_name left_ptr"
startup := Shell "xsetroot -solid #fffeeb"
startup := Shell "xset r rate 200 80"
startup := Shell "redshift"

[MaximizeChrome]
match := MatchClientClass ":client:" "chrome"
managed := Maximize ":client:"
